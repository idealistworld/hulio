{
    "name": "hulio",
    "description": "Your Friend that Does Crypto",
    "version": "1.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "./images/16.png",
        "48": "./images/48.png",
        "128": "./images/128.png"
    },
    "content_scripts": [
        {
            "js": ["tutorials/jup.js", 
                "settings/jquery-3.6.0.js"
            ],
            "css": ["tutorials/jup.css"],
            "matches": ["https://jup.ag/*"]
        },
        {
            "matches": [
                "*://*/*"
            ],
            "run_at": "document_end",
            "css": [
                "popup/popupSafe.css",
                "popup/popupWarn.css",
                "popup/popupWarnRetype.css",
                "main.css"
            ],
            "js": [
                "detection/detection.js",
                "matching/matching.js",
                "popup/popupSafe.js",
                "popup/popupWarn.js",
                "popup/popupWarnRetype.js",
                "main.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "extension/extension.html",
        "default_icon": "./images/128.png"
    },
    "options_page": "settings/settings.html",
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "images/*"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ]
}